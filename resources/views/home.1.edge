    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.mask/1.14.10/jquery.mask.js" crossorigin="anonymous"></script>

    <script>
        $(document).ready(function(){
          $('input[name="date"]').mask('00/00/0000');
          $('input[name="postal-code"]').mask('S0S 0S0');
          $('input[name="phone-number"]').mask('(00) 0000 - 0000');
          $('input[name="celular-number"]').mask('(00) 0000 - 00000');
          $('input[name="cpf"]').mask('000.000.000-00');
          $('input[name="cep"]').mask('00.000-000');

          $('input[name="postal-code"]').focusout(function() {
            $('input[name="postal-code"]').val( this.value.toUpperCase() );
          });

        })

    </script>

